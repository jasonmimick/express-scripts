        <s:element maxOccurs="unbounded" minOccurs="0" name="Referral" nillable="true" type="Referral"/>
    </s:sequence>
</s:complexType>
<s:complexType name="Referral">
    <s:sequence>
        <s:element minOccurs="0" name="ActionCode" type="s:string">

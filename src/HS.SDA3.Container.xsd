</s:complexType>
<s:complexType name="ArrayOfHealthFundHealthFund">
    <s:sequence>
        <s:element maxOccurs="unbounded" minOccurs="0" name="HealthFund" nillable="true" type="HealthFund"/>
    </s:sequence>
</s:complexType>
<s:complexType name="HealthFund">
    <s:sequence>
        <s:element minOccurs="0" name="CustomPairs" type="ArrayOfNVPairNVPair">
            <s:annotation>
                <s:documentation>The generic container to hold all sorts of data about this object
HSDB: Streamlet&lt;br&gt;</s:documentation>
            </s:annotation>
        </s:element>
        <s:element minOccurs="0" name="HealthFund" type="HealthFundCode">
            <s:annotation>
                <s:documentation>Code for health fund that provides the insurance&lt;br&gt;
SDATableName=HealthFund, HSDBClass=User.HS.ARCInsuranceType&lt;br&gt;
HL7:  IN1-3 : InsuranceCompany&lt;br&gt;
HL7:  IN1-4 : InsuranceCompanyName&lt;br&gt;
HSDB: User.HS.PAAdmInsurance(INSInsTypeDR)&lt;br&gt;
.Address&lt;br&gt;
HL7:  IN1-5 : InsuranceCompanyAddress&lt;br&gt;
.Address.Street&lt;br&gt;
HL7:  IN1-5.1 : StreetAddressStreetorMailingAddress&lt;br&gt;
HL7:  IN1-5.2 : OtherDesignation&lt;br&gt;
.Address.City&lt;br&gt;
HL7:  IN1-5.3 : City&lt;br&gt;
.Address.State&lt;br&gt;
HL7:  IN1-5.4 : StateorProvince&lt;br&gt;
.Address.Zip&lt;br&gt;
HL7:  IN1-5.5 : ZiporPostalCode&lt;br&gt;
.Address.Country&lt;br&gt;
HL7:  IN1-5.6 : Country&lt;br&gt;
.ContactPerson&lt;br&gt;
HL7:  IN1-6 : InsuranceCoContactPerson&lt;br&gt;
.ContactPerson.FamilyName&lt;br&gt;
HL7:  IN1-6.1.1 : FamilyNameSurname&lt;br&gt;
.ContactPerson.FamilyNamePrefix&lt;br&gt;
HL7:  IN1-6.1.2 : FamilyNameOwnSurnamePrefix&lt;br&gt;
.ContactPerson.GivenName&lt;br&gt;
HL7:  IN1-6.2 : GivenName&lt;br&gt;
.ContactPerson.MiddleName&lt;br&gt;
HL7:  IN1-6.3 : SecondandFurtherGivenNames&lt;br&gt;
.ContactPerson.NameSuffix&lt;br&gt;
HL7:  IN1-6.4 : Suffix&lt;br&gt;
.ContactPerson.NamePrefix&lt;br&gt;
HL7:  IN1-6.5 : Prefix&lt;br&gt;
.ContactPerson.ProfessionalSuffix&lt;br&gt;
HL7:  IN1-6.14 : ProfessionalSuffix&lt;br&gt;
.ContactInfo&lt;br&gt;
HL7:  IN1-7 : InsuranceCoPhoneNumber&lt;br&gt;
Same algorithm as for PID-14&lt;br&gt;
.ContactInfo.WorkPhoneNumber&lt;br&gt;

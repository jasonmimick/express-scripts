                    </s:annotation>
                    <s:simpleType>
                        <s:restriction base="s:string">
                            <s:enumeration value="CLINIC"/>
                            <s:enumeration value="ER"/>
                            <s:enumeration value="DEPARTMENT"/>
                            <s:enumeration value="WARD"/>
                            <s:enumeration value="OTHER"/>
                        </s:restriction>
                    </s:simpleType>
                </s:element>
            </s:sequence>
        </s:extension>
